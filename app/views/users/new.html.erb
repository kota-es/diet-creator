<%= form_for @user do |f| %>
  <%= f.text_field :name, placeholder:"name" %>
  <%= f.email_field :email, placeholder:"email" %>
  <%= f.password_field :password, placeholder:"pass" %>
  <%= f.password_field :password_confirmation, placeholder:"pass" %>
  <%= f.file_field :image %>
  <%= f.submit "送信" %>
<% end %>

